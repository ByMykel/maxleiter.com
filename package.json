{
  "name": "blog",
  "version": "1.0.0",
  "scripts": {
    "lint": "next lint && prettier --write '**/*.{js,ts,tsx}' --loglevel warn",
    "dev": "next dev",
    "build": "yarn rss && yarn sitemap && next build",
    "rss": "ts-node --project ./node.tsconfig.json scripts/rss",
    "sitemap": "ts-node --project ./node.tsconfig.json scripts/sitemap",
    "analyze": "ANALYZE=true yarn build",
    "start": "next start"
  },
  "pre-commit": "lint",
  "license": "MIT",
  "dependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "@supabase/supabase-js": "1.35.4",
    "lodash.debounce": "^4.0.8",
    "next": "^12.1.7-canary.6",
    "next-remote-watch": "^1.0.0",
    "next-themes": "^0.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rss-parser": "^3.12.0",
    "scrape-youtube": "2.2.0",
    "sitemap": "^7.1.1",
    "swr": "1.3.0",
    "youtubei": "0.0.1-rc.36"
  },
  "devDependencies": {
    "@types/marked": "4.0.3",
    "@types/node": "^17.0.12",
    "@types/prettier": "2.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/rss": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "critters": "^0.0.16",
    "dotenv": "^16.0.1",
    "eslint": "8.19.0",
    "eslint-config-next": "^12.0.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gray-matter": "^4.0.3",
    "marked": "4.0.17",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-hover-media-feature": "^1.0.1",
    "postcss-preset-env": "7.7.2",
    "prettier": "2.7.1",
    "prism-react-renderer": "1.3.5",
    "rss": "^1.2.2",
    "ts-node": "10.8.2",
    "typescript": "4.7.4",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@components": "components/",
      "@data": "data/",
      "@lib": "lib/",
      "@styles": "styles/"
    },
    "include": [
      "components",
      "lib"
    ]
  }
}
