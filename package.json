{
  "name": "blog",
  "version": "1.0.0",
  "scripts": {
    "lint": "next lint && prettier --write '**/*.js' --loglevel warn",
    "dev": "next -p 3000",
    "build": "yarn rss && yarn sitemap && next build",
    "rss": "ts-node --project ./node.tsconfig.json scripts/rss",
    "sitemap": "ts-node --project ./node.tsconfig.json scripts/sitemap"
  },
  "pre-commit": "lint",
  "license": "MIT",
  "dependencies": {
    "next": "^12.1.0",
    "react": "^18.0.0-rc.0-next-27b569969-20220211",
    "react-dom": "^18.0.0-rc.0-next-27b569969-20220211",
    "rss-parser": "^3.12.0",
    "sitemap": "^7.1.1",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@types/marked": "^4.0.1",
    "@types/node": "^17.0.12",
    "@types/prettier": "^2.4.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.11",
    "@types/rss": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "critters": "^0.0.16",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.4",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gray-matter": "^4.0.3",
    "marked": "^4.0.10",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-hover-media-feature": "^1.0.1",
    "postcss-preset-env": "^7.2.3",
    "prettier": "^2.3.2",
    "prism-react-renderer": "^1.2.1",
    "rss": "^1.2.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.4.2",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@components": "components/",
      "@data": "data/",
      "@lib": "lib/",
      "@styles": "styles/"
    },
    "include": [
      "components",
      "lib"
    ]
  }
}
